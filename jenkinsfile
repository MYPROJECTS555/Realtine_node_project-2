pipeline {
    agent any
    tools {
        
       jdk 'java-11'
       maven 'maven'
    }
    stages {
        stage('Checkout') {
            steps {
                
                git branch:'main',url:'https://github.com/MYPROJECTS555/SimpleJavaCodeTest.git'
            }
        }
        stage('maven-compile')
        {
            steps
            {
                sh '''
                   mvn compile 
                  '''
             }
        }
        stage('build')
        {
            steps
            {
                sh '''
                   mvn clean install
                  '''
            }
        }
}
}
