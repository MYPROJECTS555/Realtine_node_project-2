pipeline
{
agent 
{
lable 'Prod'
}
stages
{
stage('git-clone')
   {
     steps
         { 
          git branch :'main', url:'https://github.com/MYPROJECTS555/Realtine_node_project-2.git'
          }
   }
stage('docker-container')
 {
  steps
      {
      sh ' docker build -t nameisvikas/node:version . '
      }
 }
stage('create container')
 {
  steps
      {
       sh ' docker run -it -d --name container-1 -p 3000:3000  nameisvikas/node:version '
      }
}
}
}
